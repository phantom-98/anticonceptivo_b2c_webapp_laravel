{
    "name": "willywes/apiresponse",
    "description": "Class to generate a standard structure for api json responses",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Isla",
            "email": "alejandro.isla.c@gmail.com",
            "homepage": "http://alejandroisla.cl"
        }
    ],
    "homepage": "https://github.com/Willywes/ApiResponse",
    "keywords": ["Laravel", "ApiResponse", "Api", "Lumen", "Api Response", "Standard Api Response"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Willywes\\ApiResponse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Willywes\\ApiResponse\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Willywes\\ApiResponse\\ApiResponseServiceProvider"
            ],
            "aliases": {
                "ApiResponse": "Willywes\\ApiResponse\\Facades\\ApiResponse"
            }
        }
    }
}
